-- update and draw menu

function update_menu()
		if btnp(5) then
				scene="game"
		end
end

function draw_menu()
		cls()
		print("bullet blaze",42, 25,8)
		spr(128, 42, 55)
		spr(128, 52, 65)
		spr(128, 52, 45)
		spr(128, 62, 55)
		spr(128, 72, 45)
		spr(128, 72, 65)
		spr(128, 82, 55)
		print("press x to start",35,95,8)
end
