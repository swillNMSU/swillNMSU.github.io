-- main update and draw
function _update()
		if scene=="menu" then
				update_menu()
		elseif scene=="game" then
				update_game()
		end
end

function _draw()
		if scene=="menu" then
				draw_menu()
		elseif scene=="game" then
				draw_game()
		end
end
